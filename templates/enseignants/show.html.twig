{% extends 'base.html.twig' %}

{% block title %}Enseignants{% endblock %}

{% block body %}
    <h1>Enseignants</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ enseignant.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ enseignant.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ enseignant.prenom }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ enseignant.telephone }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ enseignant.createdAt ? enseignant.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_enseignants_index') }}">back to list</a>

    <a href="{{ path('app_enseignants_edit', {'id': enseignant.id}) }}">edit</a>

    {{ include('enseignants/_delete_form.html.twig') }}
{% endblock %}
