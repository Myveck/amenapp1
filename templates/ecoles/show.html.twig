{% block title %}Ecoles{% endblock %}

{% block body %}
    <h1>Ecoles</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ecole.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ ecole.nom }}</td>
            </tr>
            <tr>
                <th>School_name</th>
                <td>{{ ecole.schoolName }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ ecole.adresse }}</td>
            </tr>
            <tr>
                <th>Boite_postale</th>
